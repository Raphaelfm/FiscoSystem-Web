@model FiscoSystem.Models.CfopModels.CfopModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes CFOP</h1>

<div>
    <h4>Detalhes do CFOP</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Codigo CFOP
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cfop)
        </dd>
        <dt class = "col-sm-2">
            Descrição
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
        <dt class = "col-sm-2">
            Descrição Completa
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescricaoCompleta)
        </dd>
        <dt class = "col-sm-2">
            Versão
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Versao)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" type="button" class="btn btn-danger">Voltar</a>
</div>
