@model FiscoSystem.Models.NcmModels.NcmModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes NCM</h1>

<div>
    <h4>Detalhes do NCM</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Codigo do NCM
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodigoNcm)
        </dd>
        <dt class = "col-sm-2">
            Descrição do NCM
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescricaoNcm)
        </dd>  
        <dt class = "col-sm-2">
            Terminal da Importação
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Terminal)
        </dd>
        <dt class = "col-sm-2">
            Data da Importação
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Data)
        </dd>
        <dt class = "col-sm-2">
            Nível 1
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nivel1)
        </dd>
        <dt class = "col-sm-2">
            Nível 2
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nivel2)
        </dd>
        <dt class = "col-sm-2">
            Nível 3
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nivel3)
        </dd>
        <dt class = "col-sm-2">
            Versão
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Versao)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" type="button" class="btn btn-danger">Voltar</a>
</div>
